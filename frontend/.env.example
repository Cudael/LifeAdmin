# LifeAdmin Frontend Configuration
# Copy this file to .env and fill in your values

# Backend API URL
# IMPORTANT: This must match the URL where your backend is accessible
# Examples:
# - Local development: http://localhost:8000
# - Production: https://api.yourdomain.com
# - Deployed backend: https://your-backend-url.com
VITE_API_URL=http://localhost:8000

# App name (optional)
VITE_APP_NAME=LifeAdmin

# Enable Google OAuth (optional)
# Set to 'true' if Google OAuth is configured in backend
VITE_ENABLE_GOOGLE_OAUTH=true
